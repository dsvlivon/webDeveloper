<h1>Listado de artistas</h1>


<a href="/alta" class= "btn btn-primary">Agregar</a>


<table class="table">
    <tr>
        <th>Nombre</th>
        <th>Apellido</th>
        <th>Op1</th>
        <th>Op2</th>
    </tr>
    {{#each listado}}
        <tr>
            <td>{{this.nombre}}</td>
            <td>{{this.apellido}}</td>
            <td><a href="/borrar/{{this._id}}"  class= "btn btn-primary">Borrar</a></td>
            <td><a href="/modificar/{{this._id}}"  class= "btn btn-primary">Modificar</a></td>
        </tr>
    {{/each}}
</table>